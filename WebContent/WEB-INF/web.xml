<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>testjsp</display-name>
  <servlet>
    <servlet-name>add</servlet-name>
    <servlet-class>model.addDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>add</servlet-name>
    <url-pattern>/addData</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>showTable</servlet-name>
    <servlet-class>model.searchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showTable</servlet-name>
    <url-pattern>/display</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>searchBarName</servlet-name>
    <servlet-class>model.searchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>searchBarName</servlet-name>
    <url-pattern>/searchName</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>searchBarEmail</servlet-name>
    <servlet-class>model.searchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>searchBarEmail</servlet-name>
    <url-pattern>/searchEmail</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>out</servlet-name>
    <servlet-class>model.logoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>out</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
 
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>